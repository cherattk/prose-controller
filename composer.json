{
    "name": "cherattk/slim-3-starter",
    "description": "Project starter for Slim Framework v3.",
    "type": "project",
    "keywords": ["project starter" , "structure", "controller", "service", "boilerplate"],
    "version": "1.1.1",
    "license": "MIT",
    "authors": [{
            "name": "cheratt karim"
        }],
    "require": {
        "slim/slim": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {            
            "App\\": "src/App",
            "Core\\": "src/Core"
        }
    },
    "autoload-dev": {
        "psr-4": {            
            "Core\\Test\\": "tests/fixture",
            "App\\Test\\": "tests/fixture"
        }
    }
}
